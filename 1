#include <iostream>
#include <vector>

int main(){
    int n, maxdel = 1, k;
    std::cin >> n >> k;
    std::vector <int> a(n);
        for (int j = 0; j < n; j++) {
            std::cin >> a[j];
        }
        for (int i = 0; i < n; i++) {
            if (k% a[i] == 0) {
                if (a[i] > maxdel) {
                    maxdel = a[i];
                }
            }
        }
        std::cout << k/maxdel;
        return 0;
